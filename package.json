{
	"name": "@aknakos/sveltekit-tree-hierarchy",
	"version": "0.0.2",
	"homepage": "https://github.com/aknakos/sveltekit-tree-hierarchy#readme",
	"repository": "github:aknakos/sveltekit-tree-hierarchy",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"tree",
		"hierarchy"
	],
	"exports": {
		".": "./treeView/Tree.svelte"
	},
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"pp": "pnpm build && pnpm publish ./package --no-git-checks --access public"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.33.1",
		"@typescript-eslint/parser": "^5.33.1",
		"autoprefixer": "^10.4.8",
		"autosuggest-highlight": "^3.3.4",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-dnd-action": "^0.9.19",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.9"
	},
	"type": "module"
}
